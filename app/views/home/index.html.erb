<% if @error %>
	Error From API "Refresh the page"
	<%= @error %>
<% else %>
Male:
	<table border="1px">
		<tr>
			<td>Nationality</td>
			<td>0-30</td>
			<td>30-50</td>
			<td>50 and above</td>
		</tr>
		<tr>
			<td><%= @male.count %></td>
			<td><%= @male.where("data->'dob'->>'age' > ?", "0").where("data->'dob'->>'age' < ?", "31") %></td>
			<td><%= @male.where("data->'dob'->>'age' > ?", "30").where("data->'dob'->>'age' < ?", "51") %></td>
			<td><%= @male.where("data->'dob'->>'age' > ?", "50") %></td>
		</tr>
	</table>
Female:
<table border="1px">
		<tr>
			<td>Nationality</td>
			<td>0-30</td>
			<td>30-50</td>
			<td>50 and above</td>
		</tr>
		<tr>
			<td><%= @female.count %></td>
			<td><%= @female.where("data->'dob'->>'age' > ?", "0").where("data->'dob'->>'age' < ?", "31") %></td>
			<td><%= @female.where("data->'dob'->>'age' > ?", "30").where("data->'dob'->>'age' < ?", "51") %></td>
			<td><%= @female.where("data->'dob'->>'age' > ?", "50") %></td>
		</tr>
	</table>
	<br>
	<br>
	<br>
	<br>
	<br>
	For Debug:
	<%= @result["results"][0] %>
<% end %>

